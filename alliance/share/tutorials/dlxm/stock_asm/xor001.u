	;  ###--------------------------------------------------------###
	;  #   logical exclusive or					#
	;  ###--------------------------------------------------------###

data1		equ	X"1234db11"
data2		equ	X"0089dac0"

		org	X"7fffff00"
		start	init
init:
		loadi	r10, data1
		loadi	r11, data2
		loadi	r13, (data1 ^ data2)

		xor	r12 , r10 , r11
		sne	r31 , r13 , r12
		bnez	r31 , bad
		nop
		j	good
		nop

		org	X"7ffffff0"
good		j	good
		nop
bad		j	bad
		nop
		end
