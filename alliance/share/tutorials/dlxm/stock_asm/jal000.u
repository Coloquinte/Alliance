const		equ	X"14feb445"

		org	X"7fffff00"
		start	init

init:
		loadi	r23, const
		loadi	r24, (4 * const)

		jal	mul2
		nop
		jal	mul2
		nop

		seq	r1 , r23, r24
		beqz	r1 , bad
		nop
		j	good
		nop

mul2 :
		add	r23, r23, r23
		jr	r31
		nop

		org	X"7ffffff0"
good		j	good
		nop
bad		j	bad
		nop
		end
