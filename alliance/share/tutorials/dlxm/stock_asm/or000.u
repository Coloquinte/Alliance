const1		equ	X"a5de56457"
const2		equ	X"878f0105b"

		org	X"7fffff00"
		start	init

init:
		loadi	r14, const1
		loadi	r19, const2
		loadi	r9 , (const1 | const2)

		or	r20, r14, r19
		seq	r7 , r9 , r20
		bnez	r7      , good
		nop
		j	bad
		nop

		org	X"7ffffff0"
good		j	good
		nop
bad		j	bad
		nop
		end
