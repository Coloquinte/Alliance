const1		equ	X"5555"
const2		equ	X"aaaa"

		org	X"7fffff00"
		start	init

init:
		ori	r13, r0 , const1
		ori	r21, r0 , const2
		ori	r22, r0 , (const1 | const2)

		or	r23, r13, r21

		seq	r20, r23, r22
		bnez	r20     , good
		nop
		j	bad
		nop

		org	X"7ffffff0"
good		j	good
		nop
bad		j	bad
		nop
		end
