	;  ###--------------------------------------------------------###
	;  #   conditional branch					#
	;  ###--------------------------------------------------------###

val1		equ	X"000057a1"
val2		equ	X"3409a339"

		org	X"7fffff00"
		start	init

init :
		loadi	r7 , VAL1
		loadi	r8 , VAL2
		loadi	r9 , (VAL1 - VAL2)

		sub	r10 , r7 , r8

		sne	r20 , r9 , r10
		bnez	r20 , bad
		nop
		j	good
		nop

		org	X"7ffffff0"
good		j	good
		nop
bad		j	bad
		nop
		end
