	;  ###--------------------------------------------------------###
	;  #   set if less or equal					#
	;  ###--------------------------------------------------------###

data		equ	4907

		org	X"7fffff00"
		start	init

init:
		addi	r13, r0 ,  data
		slei	r14, r13, (data - 1)
		beqz	r14, good
		nop
		j	bad
		nop

		org	x"7ffffff0"
good		j	good
		nop
bad		j	bad
		nop
		end
