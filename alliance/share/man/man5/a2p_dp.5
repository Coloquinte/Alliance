i1	:	cin	=	43	(fF)
.br
		tphh	=	793	(ps) +	1699	(ohms) x Cload (pF)
.br
		tpll	=	1293	(ps) +	1314	(ohms) x Cload (pF)
.br

i0	:	cin	=	44	(fF)
.br
		tphh	=	858	(ps) +	1699	(ohms) x Cload (pF)
.br
		tpll	=	1197	(ps) +	1314	(ohms) x Cload (pF)
.br
