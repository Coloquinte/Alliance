.TH A2P_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBa2p_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "a2p_dp", "insname", "i0", "i1", "o", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  a2p_dp  IS
  PORT(
         i0 : in        BIT;
         i1 : in        BIT;
          o :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  a2p_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  a2p_dp  IS

BEGIN
  -- Logical equations.
  o     <= (i0 and i1);
END  behavior_data_flow;
.fi
.ft R

\fIPhysical features :\fP
.br
.ta +4n
	Cell wide : 24 lambdas, or 4 pitchs.
.br

\fITiming characteristics :\fP
.br
