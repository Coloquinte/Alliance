.TH NO3_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBno3_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "no3_dp", "insname", "i0", "i1", "i2", "o", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  no3_dp  IS
  PORT(
         i0 : in        BIT;
         i1 : in        BIT;
         i2 : in        BIT;
          o :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  no3_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  no3_dp  IS

BEGIN
  -- Logical equations.
  o     <= not ((i0 or i1) or i2);
END  behavior_data_flow;
.fi
.ft R

