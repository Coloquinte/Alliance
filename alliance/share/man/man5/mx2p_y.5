.TH MX2P_Y 5 "October 1, 1997" "ASIM/LIP6" "STANDARD-CELL LIBRARY"
.SH NAME
.B mx2p_y
.so man1/alc_origin.1
.SH SYNOPSIS
.B Genlib call :
.br
  LOINS("mx2p_y","name","i0","l0","i1","l1","t","vdd","vss",0);
.SH DESCRIPTION
.I - Interface
   entity mx2p_y is
     port(
        i0 : in bit;
        l0 : in bit;
        i1 : in bit;
        l1 : in bit;
        t : out bit;
        vdd : in bit;
        vss : in bit);
.br
.I - Logical description
.br
    t <= ((i1 and l1) or (i0 and l0));
   
.br
.I - Physical features
   number of grids : 8
   number of feed-through : 3
.br
   
.I - Timing caracteristics
.br
   i0 : 57 (fF)
.br
   l0 : 57 (fF)
.br
   i1 : 57 (fF)
.br
   l1 : 57 (fF)
.br

