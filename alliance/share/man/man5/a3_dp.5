.TH A3_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBa3_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "a3_dp", "insname", "i0", "i1", "i2", "o", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  a3_dp  IS
  PORT(
         i0 : in        BIT;
         i1 : in        BIT;
         i2 : in        BIT;
          o :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  a3_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  a3_dp  IS

BEGIN
  -- Logical equations.
  o     <= ((i0 and i1) and i2);
END  behavior_data_flow;
.fi
.ft R

\fIPhysical features :\fP
.br
.ta +4n
	Cell wide : 30 lambdas, or 5 pitchs.
.br

\fITiming characteristics :\fP
.br
.ta +6n  +2n  +6n +6nR +1n    +12nR +1n

i2	:	cin	=	33	(fF)
.br
		tphh	=	1155	(ps) +	2998	(ohms) x Cload (pF)
.br
		tpll	=	1545	(ps) +	2418	(ohms) x Cload (pF)
.br

i1	:	cin	=	33	(fF)
.br
		tphh	=	1229	(ps) +	2998	(ohms) x Cload (pF)
.br
		tpll	=	1449	(ps) +	2418	(ohms) x Cload (pF)
.br

i0	:	cin	=	33	(fF)
.br
		tphh	=	1263	(ps) +	2998	(ohms) x Cload (pF)
.br
		tpll	=	1350	(ps) +	2418	(ohms) x Cload (pF)
.br
