.TH TSN_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBtsn_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "tsn_dp", "insname", "i", "o", "v", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  tsn_dp  IS
  PORT(
          i : in        BIT;
          o :   out MUX_BIT BUS;
          v : in        BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  tsn_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  tsn_dp  IS

BEGIN
  -- Bus affectations.
  label0 :BLOCK( v = '1' )
  BEGIN
    o <= GUARDED not i;
  END BLOCK label0;
END  behavior_data_flow;
.fi
.ft R

\fIPhysical features :\fP
.br
.ta +4n
	Cell wide : 24 lambdas, or 4 pitchs.
.br

\fITiming characteristics :\fP
.br
.ta +6n  +2n  +6n +6nR +1n    +12nR +1n

v	:	cin	=	66	(fF)
.br
		tphl	=	245	(ps) +	1974	(ohms) x Cload (pF)
.br
		tphh	=	731	(ps) +	2979	(ohms) x Cload (pF)
.br

i	:	cin	=	67	(fF)
.br
		tplh	=	706	(ps) +	2979	(ohms) x Cload (pF)
.br
		tphl	=	442	(ps) +	1974	(ohms) x Cload (pF)
.br
