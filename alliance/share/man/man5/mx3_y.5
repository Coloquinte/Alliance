.pl -.4
.TH MX3_Y 5 "October 1, 1997" "ASIM/LIP6" "STANDARD-CELL LIBRARY"
.SH NAME
.B mx3_y
.so man1/alc_origin.1
.SH SYNOPSIS
.B Genlib call :
.br
  LOINS("mx3_y","name","i0","l0","i1","l1","i2","l2","t","vdd","vss",0);
.SH DESCRIPTION
.I - Interface
   entity mx3_y is
     port(
        i0 : in bit;
        l0 : in bit;
        i1 : in bit;
        l1 : in bit;
        i2 : in bit;
        l2 : in bit;
        t : out bit;
        vdd : in bit;
        vss : in bit);
.br
.I - Logical description
.br
    t <= (((i1 and l1) or (i2 and l2)) or (i0 and l0));
   
.br
.I - Physical features
   number of grids : 11
   number of feed-through : 4
.br
   
.I - Timing caracteristics
.br
   i0 : 50 (fF)
.br
   l0 : 50 (fF)
.br
   i1 : 49 (fF)
.br
   l1 : 50 (fF)
.br
   i2 : 50 (fF)
.br
   l2 : 50 (fF)
.br

