.TH OP3_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBop3_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "op3_dp", "insname", "i0", "i1", "i2", "o", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  op3_dp  IS
  PORT(
         i0 : in        BIT;
         i1 : in        BIT;
         i2 : in        BIT;
          o :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  op3_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  op3_dp  IS

BEGIN
  -- Logical equations.
  o     <= ((i0 or i1) or i2);
END  behavior_data_flow;
.fi
.ft R

\fIPhysical features :\fP
.br
.ta +4n
	Cell wide : 30 lambdas, or 5 pitchs.
.br

\fITiming characteristics :\fP
.br
.ta +6n  +2n  +6n +6nR +1n    +12nR +1n

i2	:	cin	=	59	(fF)
.br
		tphh	=	1334	(ps) +	1699	(ohms) x Cload (pF)
.br
		tpll	=	1524	(ps) +	1314	(ohms) x Cload (pF)
.br

i1	:	cin	=	60	(fF)
.br
		tphh	=	1146	(ps) +	1699	(ohms) x Cload (pF)
.br
		tpll	=	1550	(ps) +	1314	(ohms) x Cload (pF)
.br

i0	:	cin	=	60	(fF)
.br
		tphh	=	941	(ps) +	1699	(ohms) x Cload (pF)
.br
		tpll	=	1422	(ps) +	1314	(ohms) x Cload (pF)
.br
