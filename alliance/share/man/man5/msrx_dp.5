.TH MSRX_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBmsrx_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "msrx_dp", "insname", "i", "l", "c", "t", "f", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  msrx_dp  IS
  PORT(
          i : in        BIT;
          l : in        BIT;
          c : in        BIT;
          t :   out     BIT;
          f :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  msrx_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  msrx_dp  IS

  -- Internal registers.
  SIGNAL dff_s : REG_BIT REGISTER;

BEGIN
  -- Register affectations.
  label1 :BLOCK( c = '1' )
  BEGIN
    dff_s <= GUARDED '1';
  END BLOCK label1;
  label0 :BLOCK( (not l and not l'stable ) = '1' )
  BEGIN
    dff_s <= GUARDED not i;
  END BLOCK label0;

  -- Logical equations.
  f     <= dff_s;
  t     <= not dff_s;
END  behavior_data_flow;
.fi
.ft R

\fIPhysical features :\fP
.br
.ta +4n
	Cell wide : 78 lambdas, or 13 pitchs.
.br

\fITiming characteristics :\fP
.br
.ta +6n  +2n  +6n +6nR +1n    +12nR +1n

c	:	cin	=	15	(fF)
.br

l	:	cin	=	70	(fF)
.br

i	:	cin	=	43	(fF)
.br
