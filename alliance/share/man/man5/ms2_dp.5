.TH MS2_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBms2_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "ms2_dp", "insname", "di", "si", "se", "l", "t", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  ms2_dp  IS
  PORT(
         di : in        BIT;
         si : in        BIT;
         se : in        BIT;
          l : in        BIT;
          t :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  ms2_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  ms2_dp  IS

  -- Internal registers.
  SIGNAL dff_s : REG_BIT REGISTER;

BEGIN
  -- Register affectations.
  label0 :BLOCK( (not l and not l'stable ) = '1' )
  BEGIN
    dff_s <= GUARDED ((not di and not se) or (not si and se));
  END BLOCK label0;

  -- Logical equations.
  t     <= not dff_s;
END  behavior_data_flow;
.fi
.ft R

\fIPhysical features :\fP
.br
.ta +4n
	Cell wide : 90 lambdas, or 15 pitchs.
.br

\fITiming characteristics :\fP
.br
.ta +6n  +2n  +6n +6nR +1n    +12nR +1n

l	:	cin	=	68	(fF)
.br

se	:	cin	=	114	(fF)
.br

si	:	cin	=	70	(fF)
.br

di	:	cin	=	68	(fF)
.br
.SH SEE ALSO
.ft B
dplib(1), fpgen(1), scmap(1)
.ft R

