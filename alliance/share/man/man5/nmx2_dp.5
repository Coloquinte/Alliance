.TH NMX2_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBnmx2_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "nmx2_dp", "insname", "i0", "i1", "i2", "i3", "o", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  nmx2_dp  IS
  PORT(
         i0 : in        BIT;
         i1 : in        BIT;
         i2 : in        BIT;
         i3 : in        BIT;
          o :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  nmx2_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  nmx2_dp  IS

BEGIN
  -- Logical equations.
  o     <= not ((i0 and i1) or (i2 and i3));
END  behavior_data_flow;
.fi
.ft R

