.TH O2_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBo2_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "o2_dp", "insname", "i0", "i1", "o", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  o2_dp  IS
  PORT(
         i0 : in        BIT;
         i1 : in        BIT;
          o :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  o2_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  o2_dp  IS

BEGIN
  -- Logical equations.
  o     <= (i0 or i1);
END  behavior_data_flow;
.fi
.ft R

\fIPhysical features :\fP
.br
.ta +4n
	Cell wide : 24 lambdas, or 4 pitchs.
.br

\fITiming characteristics :\fP
.br
.ta +6n  +2n  +6n +6nR +1n    +12nR +1n

i1	:	cin	=	48	(fF)
.br
		tphh	=	1151	(ps) +	2998	(ohms) x Cload (pF)
.br
		tpll	=	975	(ps) +	2418	(ohms) x Cload (pF)
.br

i0	:	cin	=	48	(fF)
.br
		tphh	=	920	(ps) +	2998	(ohms) x Cload (pF)
.br
		tpll	=	1014	(ps) +	2418	(ohms) x Cload (pF)
.br
