.TH NOP3_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBnop3_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "nop3_dp", "insname", "i0", "i1", "i2", "o", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  nop3_dp  IS
  PORT(
         i0 : in        BIT;
         i1 : in        BIT;
         i2 : in        BIT;
          o :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  nop3_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  nop3_dp  IS

BEGIN
  -- Logical equations.
  o     <= not ((i0 or i1) or i2);
END  behavior_data_flow;
.fi
.ft R

\fIPhysical features :\fP
.br
.ta +4n
	Cell wide : 42 lambdas, or 7 pitchs.
.br

\fITiming characteristics :\fP
.br
.ta +6n  +2n  +6n +6nR +1n    +12nR +1n

i2	:	cin	=	60	(fF)
.br
		tplh	=	1789	(ps) +	1699	(ohms) x Cload (pF)
.br
		tphl	=	1241	(ps) +	1314	(ohms) x Cload (pF)
.br

i1	:	cin	=	60	(fF)
.br
		tplh	=	1924	(ps) +	1699	(ohms) x Cload (pF)
.br
		tphl	=	1457	(ps) +	1314	(ohms) x Cload (pF)
.br

i0	:	cin	=	60	(fF)
.br
		tplh	=	1900	(ps) +	1699	(ohms) x Cload (pF)
.br
		tphl	=	1651	(ps) +	1314	(ohms) x Cload (pF)
.br
