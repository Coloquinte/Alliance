.TH MX2_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBmx2_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "mx2_dp", "insname", "i0", "l0", "i1", "l1", "t", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  mx2_dp  IS
  PORT(
         i0 : in        BIT;
         l0 : in        BIT;
         i1 : in        BIT;
         l1 : in        BIT;
          t :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  mx2_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  mx2_dp  IS

BEGIN
  -- Logical equations.
  t     <= ((l1 and i1) or (l0 and i0));
END  behavior_data_flow;
.fi
.ft R

\fIPhysical features :\fP
.br
.ta +4n
	Cell wide : 42 lambdas, or 7 pitchs.
.br

\fITiming characteristics :\fP
.br
