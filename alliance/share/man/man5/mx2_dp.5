l1	:	cin	=	44	(fF)
.br
		tphh	=	1134	(ps) +	2998	(ohms) x Cload (pF)
.br
		tpll	=	1431	(ps) +	2418	(ohms) x Cload (pF)
.br

i1	:	cin	=	44	(fF)
.br
		tphh	=	1049	(ps) +	2998	(ohms) x Cload (pF)
.br
		tpll	=	1530	(ps) +	2418	(ohms) x Cload (pF)
.br

l0	:	cin	=	44	(fF)
.br
		tphh	=	1529	(ps) +	2998	(ohms) x Cload (pF)
.br
		tpll	=	1510	(ps) +	2418	(ohms) x Cload (pF)
.br

i0	:	cin	=	44	(fF)
.br
