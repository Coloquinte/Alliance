.TH NXR2_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBnxr2_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "nxr2_dp", "insname", "i0", "i1", "o", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  nxr2_dp  IS
  PORT(
         i0 : in        BIT;
         i1 : in        BIT;
          o :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  nxr2_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  nxr2_dp  IS

BEGIN
  -- Logical equations.
  o     <= not ((i0 and not i1) or (i1 and not i0));
END  behavior_data_flow;
.fi
.ft R

