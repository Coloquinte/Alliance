.TH NOUE4_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBnoue4_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "noue4_dp", "insname", "i0", "i1", "i2", "i3", "f", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  noue4_dp  IS
  PORT(
         i0 : in        BIT;
         i1 : in        BIT;
         i2 : in        BIT;
         i3 : in        BIT;
          f :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  noue4_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  noue4_dp  IS

BEGIN
  -- Logical equations.
  f     <= not ((i2 and i3) and (i0 or i1));
END  behavior_data_flow;
.fi
.ft R

\fIPhysical features :\fP
.br
.ta +4n
	Cell wide : 30 lambdas, or 5 pitchs.
.br

