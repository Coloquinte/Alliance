.TH CRY_Y 5 "October 1, 1997" "ASIM/LIP6" "STANDARD-CELL LIBRARY"
.SH NAME
.B cry_y
.so man1/alc_origin.1
.SH SYNOPSIS
.B Genlib call :
.br
  LOINS("cry_y","name","pi","ci","si","f","vdd","vss",0);
.SH DESCRIPTION
.I - Interface
   entity cry_y is
     port(
        pi : in bit;
        ci : in bit;
        si : in bit;
        f : out bit;
        vdd : in bit;
        vss : in bit);
.br
.I - Logical description
.br
    f <= not ((si and ci) or (pi and (si or ci)));
   
.br
.I - Physical features
   number of grids : 6
   number of feed-through : 2
.br
   
.I - Timing caracteristics
.br
   pi : 102 (fF)
.br
   ci : 99 (fF)
.br
   si : 53 (fF)
.br

