.TH NDRV_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBndrv_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "ndrv_dp", "insname", "i", "o", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  ndrv_dp  IS
  PORT(
          i : in        BIT;
          o :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  ndrv_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  ndrv_dp  IS

BEGIN
  -- Logical equations.
  o     <= not i;
END  behavior_data_flow;
.fi
.ft R

\fIPhysical features :\fP
.br
.ta +4n
	Cell wide : 18 lambdas, or 3 pitchs.
.br

\fITiming characteristics :\fP
.br
.ta +6n  +2n  +6n +6nR +1n    +12nR +1n

i	:	cin	=	133	(fF)
.br
		tplh	=	399	(ps) +	893	(ohms) x Cload (pF)
.br
		tphl	=	207	(ps) +	724	(ohms) x Cload (pF)
.br
