.TH NAO4_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBnao4_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "nao4_dp", "insname", "i", "i0", "i1", "i2", "f", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  nao4_dp  IS
  PORT(
          i : in        BIT;
         i0 : in        BIT;
         i1 : in        BIT;
         i2 : in        BIT;
          f :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  nao4_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  nao4_dp  IS

BEGIN
  -- Logical equations.
  f     <= (not i and not ((i0 and i1) and i2));
END  behavior_data_flow;
.fi
.ft R

