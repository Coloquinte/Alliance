i1	:	cin	=	61	(fF)
.br
		tplh	=	572	(ps) +	2979	(ohms) x Cload (pF)
.br
		tphl	=	626	(ps) +	2418	(ohms) x Cload (pF)
.br

i0	:	cin	=	61	(fF)
.br
		tplh	=	607	(ps) +	2979	(ohms) x Cload (pF)
.br
		tphl	=	410	(ps) +	2418	(ohms) x Cload (pF)
.br
