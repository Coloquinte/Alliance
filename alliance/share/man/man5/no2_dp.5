.TH NO2_DP 5 "October 1, 1997" "ASIM/LIP6" "FITPATH - User Cell Library"
.so man1/alc_origin.1
.SH NAME
\fBno2_dp\fP
.SH SYNOPSIS
.ft CR
.nf
\fIGenLib Call :\fP
    LOINS( "no2_dp", "insname", "i0", "i1", "o", "vdd", "vss", 0L );
.fi
.ft R
.SH DESCRIPTION
\fIVHDL Behavioral description :\fP
.ft CR
.nf

-- Interface declaration.
ENTITY  no2_dp  IS
  PORT(
         i0 : in        BIT;
         i1 : in        BIT;
          o :   out     BIT;
        vdd : in        BIT;
        vss : in        BIT
      );
END  no2_dp;

-- Architecture declaration.
ARCHITECTURE  behavior_data_flow  OF  no2_dp  IS

BEGIN
  -- Logical equations.
  o     <= not (i0 or i1);
END  behavior_data_flow;
.fi
.ft R

\fIPhysical features :\fP
.br
.ta +4n
	Cell wide : 18 lambdas, or 3 pitchs.
.br

\fITiming characteristics :\fP
.br
