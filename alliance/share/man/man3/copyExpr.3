.TH COPYEXPR 3 "October 1, 1997" "ASIM/LIP6" "ABL functions"
.so man1/alc_origin.1
.SH NAME
\fBcopyExpr\fP \- copies an expression 
.SH SYNOPSYS
.nf
#include "logmmm.h"
chain_list *copyExpr(expr)
chain_list *expr;
.fi
.SH PARAMETER
.TP 20
\fIexpr\fP
expression to copy
.SH DESCRIPTION
\fBcopyExpr()\fP creates a new ABL wich is strictly equal to \fIexpr\fP.
.SH EXAMPLE
.nf
#include "mutnnn.h"		/* mbk utilities */
#include "logmmm.h"
chain_list *expr1,*expr2; 
expr1 = createExpr(OR);
addQExpr(expr1,createAtom("a"));
addQExpr(expr1,createAtom("b"));
expr2 = copyExpr(expr1);
displayExpr(expr1);
displayExpr(expr2);
/* it will display
(OR a b)
(OR a b)
*/
freeExpr(expr1);
freeExpr(expr2);
.fi
