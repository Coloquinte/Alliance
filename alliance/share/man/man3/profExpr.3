.TH PROFEXPR 3 "October 1, 1997" "ASIM/LIP6" "ABL functions"
.so man1/alc_origin.1
.SH NAME
\fBprofExpr\fP \- returns the depth of an expression. 
.SH SYNOPSYS
.nf
#include "logmmm.h"
int profExpr(expr)
chain_list *expr;
.fi
.SH PARAMETER
.TP 20
\fIexpr\fP
expression
.SH DESCRIPTION
\fBprofExpr()\fP returns the depth of \fIexpr\fP.  
.SH EXAMPLE
.nf
#include "mutnnn.h"		/* mbk utilities */
#include "logmmm.h"
chain_list *expr1,*expr2;
expr1 = createExpr(OR);
addQExpr(expr1,createAtom("a"));
addQExpr(expr1,createAtom("b"));
expr2 = createAtom("a");
printf("%d",profExpr(expr2));
printf("%d",profExpr(expr1));
expr = notExpr(expr1);
printf("%d",profExpr(expr1));
/* it will display
0
1
2
*/
freeExpr(expr1);
freeExpr(expr2);
.fi
