entity op2_y is
   port (
      i0  : in      bit;
      i1  : in      bit;
      t   : out     bit;
      vdd : in      bit;
      vss : in      bit
 );
end op2_y;

architecture structural of op2_y is
Component o2_x4
   port (
      i0  : in      bit;
      i1  : in      bit;
      q   : out     bit;
      vdd : in      bit;
      vss : in      bit
 );
end component;


begin

u4 : o2_x4
   port map (
      i0  => i1,
      i1  => i0,
      q   => t,
      vdd => vdd,
      vss => vss
   );


end structural;
