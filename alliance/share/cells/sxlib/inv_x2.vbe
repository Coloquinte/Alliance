ENTITY inv_x2 IS
GENERIC (
  CONSTANT area 	 : NATURAL := 750;
  CONSTANT transistors	 : NATURAL := 2;
  CONSTANT cin_i	 : NATURAL := 12;
  CONSTANT tplh_i_nq	 : NATURAL := 162;
  CONSTANT rup_i_nq	 : NATURAL := 2410;
  CONSTANT tphl_i_nq	 : NATURAL := 68;
  CONSTANT rdown_i_nq	 : NATURAL := 1600
);
PORT (
  i	 : in  BIT;
  nq	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END inv_x2;

ARCHITECTURE behaviour_data_flow OF inv_x2 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on inv_x2"
  SEVERITY WARNING;
  nq <= not (i) after 700 ps;
END;
