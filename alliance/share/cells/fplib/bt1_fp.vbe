ENTITY bt1_fp IS
PORT (
     i    :  IN BIT ;
     en   : OUT BIT ;
     enb  : OUT BIT ;
     vdd  :  IN BIT ;
     vss  :  IN BIT ;
     vss1 :  IN BIT
     );
END bt1_fp ;

ARCHITECTURE behaviour_data_flow OF bt1_fp IS
BEGIN
en <= i ;
enb <= not i ;
ASSERT ((vdd='1') and (vss='0') and (vss1
='0'))
REPORT "Power supply is missing on bt1_fp "
SEVERITY WARNING ;
END;

