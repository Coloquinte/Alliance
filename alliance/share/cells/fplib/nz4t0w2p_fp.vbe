-- Entity Declaration.

ENTITY  nz16t0w2p_fp  IS
	PORT(
		i3    : in    BIT;
		i2    : in    BIT;
		i1    : in    BIT;
		i0    : in    BIT;
		z16    :   out BIT;
		vdd : in    BIT;
		vdd_1 : in    BIT;
		vss : in    BIT;
		vss_1 : in    BIT;
		vss_2 : in    BIT
	);
END  nz16t0w2p_fp;


-- Architecture declaration.

ARCHITECTURE  behavior_data_flow  OF  nz16t0w2p_fp  IS


BEGIN
	z16 <= i3 or i2 or i1 or i0;

    ASSERT(vdd = '1')
    	REPORT "Power supply vdd of nz16t0w2p_fp badly connected."
	    SEVERITY WARNING;

    ASSERT(vdd_1 = '1')
    	REPORT "Power supply vdd_1 of nz16t0w2p_fp badly connected."
	    SEVERITY WARNING;

    ASSERT(vss = '0')
    	REPORT "Power supply vss of nz16t0w2p_fp badly connected."
	    SEVERITY WARNING;

    ASSERT(vss_1 = '0')
    	REPORT "Power supply vss_1 of nz16t0w2p_fp badly connected."
	    SEVERITY WARNING;

    ASSERT(vss_2 = '0')
    	REPORT "Power supply vss_2 of nz16t0w2p_fp badly connected."
	    SEVERITY WARNING;
END  behavior_data_flow;
